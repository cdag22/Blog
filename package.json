{
  "name": "blog",
  "version": "0.0.1",
  "description": "A basic isomorphic react, redux, blog app with custom webpack config",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server.bundle.js",
    "build": "npm i && webpack --config webpack.server.js && webpack --config webpack.client.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/server.bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdag22/Blog.git"
  },
  "author": "cj dagostino",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cdag22/Blog/issues"
  },
  "homepage": "https://github.com/cdag22/Blog#readme",
  "dependencies": {
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^2.1.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "css-loader": "^3.2.0",
    "faker": "^4.1.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "isomorphic-style-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.1.3",
    "webpack": "^4.41.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  }
}
